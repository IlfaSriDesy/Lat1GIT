<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="project.bersama.model.FoodModel" table="food_tbl">
        <id name="food_id" column="food_id" type="string">
            <generator class="assigned"></generator>
        </id>
        <property name="food_nm" column="food_nm" />
        <property name="food_desc" column="food_desc"/>
    </class>
  

</hibernate-mapping>
