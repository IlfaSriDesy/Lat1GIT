<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class name="project.bersama.model.FoodModel" table="food_tbl">
        <id name="foodId" column="foodId" type="string">
            <generator class="assigned" />
        </id>
        <property name="foodNm" column="foodNm"/>
        <property name="foodSpec" column="foodSpec"/>
    </class>
</hibernate-mapping>
