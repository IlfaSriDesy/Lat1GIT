<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="project.bersama.model.MemberModel" table="Member_tbl">
        <id name="member_id" column="member_id" type="string">
            <generator class="assigned" />
        </id>
        
        <property name="member_nm" column="member_nm"/>
        <property name="member_jk" column="member_jk"/>
        <property name="member_point" column="member_point"/>
    </class>
  

</hibernate-mapping>
