<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="project.bersama.model.foodProductModel" table="foodProduct_tbl">
        <id name="foodProduct_id" column="foodProduct_id" type="string">
            <generator class="assigned" />
        </id>
        <property name="foodProduct_nm" column="foodProduct_nm"/>
        <property name="foodCategory_id" column="foodCategory_id"/>
    </class>
  

</hibernate-mapping>
