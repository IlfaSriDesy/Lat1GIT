<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="project.bersama.model.transaksipoint_tbl" table="transaksi_tbl">
        <id name="trans_id" column="id" type="string">
            <generator class="assigned" />
        </id>
        <property name="date" column="date1"/>
        <property name="member_id" column="member"/>
        <property name="jnspoint_id" column="jns"/>
        <property name="desc" column="desc1"/>
    </class>
  
  

</hibernate-mapping>
